{
   "name": "@agnost/client",
   "version": "1.0.3",
   "description": "Javascript client-side (e.g., front-end app) client for Agnost backend app development platform.",
   "keywords": [
      "javascript",
      "typescript",
      "agnost",
      "backend"
   ],
   "main": "dist/index.js",
   "module": "dist/index.js",
   "types": "dist/index.d.ts",
   "files": [
      "dist",
      "src"
   ],
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "build": "tsc -p tsconfig.json",
      "buildumd": "webpack",
      "run": "node ./dist/index.js",
      "format": "prettier --write \"src/**/*.ts\"",
      "lint": "tslint -p tsconfig.json",
      "prepare": "npm run build && npm run buildumd",
      "preversion": "npm run lint",
      "version": "npm run format && git add .",
      "postversion": "git push && git push origin master",
      "docs": "typedoc src/index.ts"
   },
   "repository": {
      "type": "git",
      "url": "git@github.com:cloud-agnost/agnost-client.git"
   },
   "author": "Agnost",
   "license": "Apache 2.0",
   "bugs": {
      "url": "https://github.com/cloud-agnost/agnost-client/issues"
   },
   "homepage": "https://github.com/cloud-agnost/agnost-client",
   "devDependencies": {
      "@types/node": "^20.4.1",
      "prettier": "^3.0.0",
      "process": "^0.11.10",
      "ts-loader": "^9.4.4",
      "tslint": "^6.1.3",
      "tslint-config-prettier": "^1.18.0",
      "typedoc": "^0.24.8",
      "typedoc-plugin-missing-exports": "^2.0.0",
      "webpack": "^5.68.0",
      "webpack-cli": "^4.9.2",
      "webpack-remove-debug": "^0.1.0"
   },
   "dependencies": {
      "cross-fetch": "^4.0.0",
      "socket.io-client": "^4.7.2"
   },
   "jsdelivr": "dist/umd/agnost.js",
   "unpkg": "dist/umd/agnost.js"
}
